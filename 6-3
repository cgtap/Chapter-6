#include <iostream>
#include <string>
using namespace std;

int main() {
    // Write your main here
    char str[100];
    int v = 0;
    cout << "Enter a string : ";
    gets(str);
    for (int i = 0; str[i]!='\0'; ++i)
    {
        if (str[i] == 'a' || str[i] == 'e' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u' || str[i] == 'A' || str[i] == 'E' || str[i] == 'I' || str[i] == 'O' || str[i] == 'U')
            ++v;
    }
    cout << v << " vowels in this sentence.";
    
    return 0;
}
